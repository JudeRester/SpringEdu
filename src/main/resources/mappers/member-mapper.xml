<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="comkh.myapp.member.dao.MemberMapperDAO">
    <select id="getByMemberID" resultType="com.kh.myapp.member.vo.MemberVO"
    	parameterType="String">
    	select * from member where id = #{id}
    </select>
    
    <select id="getByMemberAll" resultType="com.kh.myapp.member.vo.MemberVO">
    	select * from member where id = #{id}
    </select>
		<insert id="memberInsert" parameterType="com.kh.myapp.member.vo.MemberVO">
			insert into member values(#{id},#{passwd},#{name},${birth},${phone},#{gender})
		</insert>
		<delete id="memberDelete" parameterType="String">
			delete from member where id = #{id}
		</delete>    
		<update id="memberUpdate" parameterType="com.kh.myapp.member.vo.MemberVO">
			update member set
			passwd=#{passwd},
			name=#{name},
			birth=${birth},
			phone=${phone}
			gender=${gender}
			where id = #{id}
		</update>
    </mapper>